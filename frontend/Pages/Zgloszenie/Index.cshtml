@page
@model IndexZgloszenieModel
@{
}

<link rel="stylesheet" href="https://unpkg.com/leaflet@1.9.4/dist/leaflet.css"
      integrity="sha256-p4NxAoJBhIIN+hmNHrzRCf9tD/miZyoHS5obTRR9BMY="
      crossorigin="" />

<script src="https://unpkg.com/leaflet@1.9.4/dist/leaflet.js"
        integrity="sha256-20nQCchB9co0qIjJZRGuk2/Z9VM+kNiyxNV1lvTlZBo="
        crossorigin=""></script>


<h1>Index</h1>

<p>
    <a href="/Zgloszenie/Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                OgloszenieId
            </th>
            <th>
                WolontariuszId
            </th>
            <th>
                Ocena
            </th>
            <th>
                Tresc
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.Zgloszenia)
        {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.OgloszenieId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.WolontariuszId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Ocena)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Tresc)
            </td>
            <td>
                <a asp-page="./Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-page="./Details" asp-route-id="@item.Id">Details</a> |
                <a asp-page="./Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
